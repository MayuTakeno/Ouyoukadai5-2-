<div class="container">
  <div class="row">
    <div class="col-md-8 offset-md-1">
      <h2>Result index</h2>
      <table>
        <% if @range == "User" %>
          <tbody>
            <% @users.each do |user| %>
            <tr>
              <td><%= user.name %></td>
            </tr>
            <% end %>
          </tbody>
          <% else %>
          <tbody>
            <% @books.each do |book| %>
              <tr>
                <td><%= book.title %></td>
                <td><%= book.body %></td>
              </tr>
            <% end %>
          </tbody>
        <% end %>
      </table>
    </div>
  </div>
</div>